{
  "name": "justto-web",
  "version": "1.10.837",
  "homepage": "https://app.justto.com",
  "author": "Henrique Liberato",
  "engines": {
    "node": "^8.11.4",
    "npm": "^5.6.0"
  },
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build && apv update build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "inspect": "vue-cli-service inspect",
    "release": "vue-cli-service build && apv update patch --tag",
    "serve:prod": "vue-cli-service serve --open --mode production",
    "styleguide": "vue-styleguidist server --open",
    "styleguide:build": "vue-styleguidist build",
    "test:server": "npx vue-cli-service test:e2e --headless --browser chrome"
  },
  "dependencies": {
    "@fnando/cnpj": "^1.0.2",
    "@fnando/cpf": "^1.0.2",
    "@sentry/browser": "^5.15.5",
    "@sentry/integrations": "^5.15.5",
    "@vue/ui": "^0.5.6",
    "appversion": "^1.7.1",
    "chart.js": "^2.9.3",
    "ckeditor4-vue": "^1.1.0",
    "core-js": "^3.6.5",
    "cpf_cnpj": "^0.2.0",
    "element-ui": "2.12.0",
    "file-saver": "^2.0.2",
    "lodash": "^4.17.19",
    "moment-timezone": "^0.5.26",
    "postcss-loader": "^3.0.0",
    "register-service-worker": "^1.6.2",
    "v-money": "^0.8.1",
    "vue": "^2.6.10",
    "vue-awesome-swiper": "^3.1.3",
    "vue-chartjs": "^3.5.0",
    "vue-chat-scroll": "^1.3.5",
    "vue-currency-filter": "^3.4.2",
    "vue-draggable-resizable": "^2.1.0",
    "vue-i18n": "^8.11.2",
    "vue-infinite-loading": "^2.4.4",
    "vue-moment": "^4.0.0",
    "vue-perfect-scrollbar": "^0.1.0",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.0.6",
    "vue-scrollable-tabs": "^1.0.5",
    "vue-socket.io": "^3.0.7",
    "vue-text-highlight": "^2.0.10",
    "vue-the-mask": "^0.11.1",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@vue/cli-plugin-babel": "^4.4.5",
    "@vue/cli-plugin-e2e-cypress": "^4.4.5",
    "@vue/cli-plugin-eslint": "^4.4.5",
    "@vue/cli-plugin-pwa": "^4.4.5",
    "@vue/cli-plugin-unit-jest": "^4.4.5",
    "@vue/cli-service": "^4.4.5",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.0.0-beta.29",
    "axios": "^0.21.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^23.6.0",
    "compression-webpack-plugin": "^3.0.0",
    "cypress-file-upload": "^3.1.4",
    "eslint": "^6.7.2",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-axios": "0.0.4",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-cli-plugin-i18n": "^1.0.1",
    "vue-cli-plugin-styleguidist": "^4.25.0",
    "vue-template-compiler": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  }
}
