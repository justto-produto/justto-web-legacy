{
  "name": "justto-web",
  "version": "2.0.180",
  "homepage": "https://justto.app",
  "author": "Justto",
  "engines": {
    "node": "^14.16.0",
    "npm": "^7.7.6"
  },
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build && apv update build",
    "build:icons": "icon-font-generator src/assets/icons/*.svg -o public/fonts --csspath public/styles/icons.css --jsonpath src/constants/icons.json --html false --types 'ttf, woff, woff2, eot' -p ic",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "list:icons": "sh listIcons.sh",
    "inspect": "vue-cli-service inspect",
    "release": "vue-cli-service build && apv update patch --tag",
    "serve:prod": "vue-cli-service serve --open --mode production",
    "styleguide": "vue-styleguidist server --open",
    "styleguide:build": "vue-styleguidist build",
    "test:server": "npx vue-cli-service test:e2e --headless --browser chrome"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-alignment": "^27.0.0",
    "@ckeditor/ckeditor5-autoformat": "^27.0.0",
    "@ckeditor/ckeditor5-block-quote": "^27.0.0",
    "@ckeditor/ckeditor5-build-classic": "^27.0.0",
    "@ckeditor/ckeditor5-clipboard": "^27.1.0",
    "@ckeditor/ckeditor5-dev-utils": "^24.4.2",
    "@ckeditor/ckeditor5-dev-webpack-plugin": "^24.4.2",
    "@ckeditor/ckeditor5-editor-classic": "^27.0.0",
    "@ckeditor/ckeditor5-essentials": "^27.0.0",
    "@ckeditor/ckeditor5-highlight": "^27.0.0",
    "@ckeditor/ckeditor5-image": "^27.1.0",
    "@ckeditor/ckeditor5-indent": "^27.0.0",
    "@ckeditor/ckeditor5-link": "^27.1.0",
    "@ckeditor/ckeditor5-list": "^27.0.0",
    "@ckeditor/ckeditor5-mention": "^27.1.0",
    "@ckeditor/ckeditor5-paragraph": "^27.0.0",
    "@ckeditor/ckeditor5-paste-from-office": "^27.1.0",
    "@ckeditor/ckeditor5-remove-format": "^27.0.0",
    "@ckeditor/ckeditor5-table": "^27.1.0",
    "@ckeditor/ckeditor5-theme-lark": "^27.0.0",
    "@ckeditor/ckeditor5-typing": "^27.0.0",
    "@ckeditor/ckeditor5-ui": "^27.0.0",
    "@ckeditor/ckeditor5-vue2": "^1.0.5",
    "@fnando/cnpj": "^1.0.2",
    "@fnando/cpf": "^1.0.2",
    "@sentry/browser": "^5.15.5",
    "@sentry/integrations": "^5.15.5",
    "@vue/ui": "^0.5.6",
    "appversion": "^1.7.1",
    "chart.js": "^2.9.3",
    "ckeditor4-vue": "^1.1.0",
    "core-js": "^3.6.5",
    "cpf_cnpj": "^0.2.0",
    "element-ui": "^2.15.1",
    "file-saver": "^2.0.2",
    "lodash": "^4.17.19",
    "moment-timezone": "^0.5.26",
    "monaco-editor": "^0.20.0",
    "monaco-editor-webpack-plugin": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^0.5.1",
    "register-service-worker": "^1.6.2",
    "v-money": "^0.8.1",
    "vue": "^2.6.10",
    "vue-awesome-swiper": "^3.1.3",
    "vue-chartjs": "^3.5.0",
    "vue-chat-scroll": "^1.3.5",
    "vue-currency-filter": "^3.4.2",
    "vue-draggable-resizable": "^2.1.0",
    "vue-html2pdf": "^1.8.0",
    "vue-i18n": "^8.11.2",
    "vue-infinite-loading": "^2.4.4",
    "vue-moment": "^4.0.0",
    "vue-monaco": "^1.2.2",
    "vue-perfect-scrollbar": "^0.1.0",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.0.6",
    "vue-scrollable-tabs": "^1.0.5",
    "vue-shortkey": "^3.1.7",
    "vue-socket.io": "^3.0.7",
    "vue-text-highlight": "^2.0.10",
    "vue-the-mask": "^0.11.1",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@ckeditor/ckeditor5-basic-styles": "^27.1.0",
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@vue/cli-plugin-babel": "^4.4.5",
    "@vue/cli-plugin-e2e-cypress": "^4.4.5",
    "@vue/cli-plugin-eslint": "^4.4.5",
    "@vue/cli-plugin-pwa": "^4.4.5",
    "@vue/cli-plugin-unit-jest": "^3.12.1",
    "@vue/cli-service": "^4.4.5",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.0.0-beta.29",
    "axios": "^0.21.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "compression-webpack-plugin": "^7.1.2",
    "cypress-file-upload": "^5.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "icon-font-generator": "^2.1.11",
    "sass": "^1.32.8",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-axios": "0.0.4",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-cli-plugin-i18n": "^1.0.1",
    "vue-cli-plugin-styleguidist": "^4.25.0",
    "vue-template-compiler": "^2.6.10"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  }
}
