{
  "name": "justto-web",
  "version": "2.0.332",
  "homepage": "https://acordos.projuris.com.br",
  "author": "ProJuris",
  "engines": {
    "node": "^14.18.2",
    "npm": "^6.14.15"
  },
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "increment-version": "npm version patch",
    "build": "vue-cli-service build --mode production --report",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "list:icons": "sh listIcons.sh",
    "inspect": "vue-cli-service inspect",
    "release": "vue-cli-service build && apv update patch --tag",
    "serve:prod": "vue-cli-service serve --open --mode production",
    "styleguide": "vue-styleguidist server --open",
    "styleguide:build": "vue-styleguidist build",
    "test:server": "npx vue-cli-service test:e2e --headless --browser chrome",
    "sonar:update-version": "node sync-sonar-version.js",
    "sonar": "npm run sonar:update-version && sonar-scanner"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-alignment": "^27.0.0",
