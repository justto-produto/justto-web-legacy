{
  "name": "justto-web",
  "version": "1.10.142",
  "homepage": "https://app.justto.com",
  "author": "Henrique Liberato",
  "engines": {
    "node": "^8.11.4",
    "npm": "^5.6.0"
  },
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "serve:prod": "vue-cli-service serve --open --mode production",
    "build": "vue-cli-service build && apv update build",
    "release": "vue-cli-service build && apv update patch --tag",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "styleguide": "vue-styleguidist server --open",
    "styleguide:build": "vue-styleguidist build",
    "inspect": "vue-cli-service inspect",
    "test:server": "npx vue-cli-service test:e2e --headless --browser chrome"
  },
  "dependencies": {
    "@vue/ui": "^0.5.6",
    "appversion": "^1.7.1",
    "cpf_cnpj": "^0.2.0",
    "element-ui": "2.12.0",
    "file-loader": "^4.2.0",
    "file-saver": "^2.0.2",
    "fuse.js": "^3.4.5",
    "moment-timezone": "^0.5.26",
    "register-service-worker": "^1.6.2",
    "v-money": "^0.8.1",
    "vue": "^2.6.10",
    "vue-awesome-swiper": "^3.1.3",
    "vue-chat-scroll": "^1.3.5",
    "vue-currency-filter": "^3.4.2",
    "vue-i18n": "^8.11.2",
    "vue-infinite-loading": "^2.4.4",
    "vue-moment": "^4.0.0",
    "vue-nestable": "^2.5.1",
    "vue-perfect-scrollbar": "^0.1.0",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.0.6",
    "vue-socket.io": "^3.0.7",
    "vue-the-mask": "^0.11.1",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-e2e-cypress": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-plugin-pwa": "^3.8.0",
    "@vue/cli-plugin-unit-jest": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-standard": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.29",
    "axios": "^0.18.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "compression-webpack-plugin": "^3.0.0",
    "cypress-file-upload": "^3.1.4",
    "eslint-plugin-cypress": "^2.2.1",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.0.3",
    "vue-cli-plugin-axios": "0.0.4",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-cli-plugin-i18n": "^0.5.2",
    "vue-cli-plugin-styleguidist": "^0.1.10",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "@vue/standard"
    ],
    "rules": {
      "standard/no-callback-literal": 0,
      "vue/max-attributes-per-line": [
        "error",
        {
          "singleline": 5,
          "multiline": {
            "max": 1,
            "allowFirstLine": false
          }
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  }
}
